source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '11.0'

target 'ChallengeProject' do
inhibit_all_warnings!

  use_frameworks!

  pod 'HandyJSON',    '5.0.0'                  #用于JSON转化为Model
  pod 'MJRefresh',    '3.2.0'                  #刷新控件
  pod 'SnapKit',      '4.2.0'                  #autolayout 布局
  pod 'Alamofire',    '4.9.0'                  #网络请求框架
  pod 'HighPrecisionCalculate-iOS', '1.0.5'    #高精度计算
  pod 'SwiftCharts'
  pod 'MBProgressHUD', '1.1.0'                 #蒙版，
  pod 'Toast', '4.0.0'                         #toast
  pod 'FMDB'                                   #数据库



  target 'ChallengeProjectTests' do
    inherit! :search_paths
  end

  target 'ChallengeProjectUITests' do
    inherit! :search_paths
  end

end
